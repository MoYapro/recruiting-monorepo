{
  "name": "vue",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "dev:mock": "vite --open --mode mock",
    "dev:spring:kotlin": "vite --open --mode kotlin",
    "dev:spring:java": "vite --open --mode java",
    "dev:csharp": "vite --open --mode c#",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "qa": "run-s lint test:run:coverage",
    "lint": "run-p -c -l format:check lint:eslint lint:tsc",
    "lint:eslint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:fix": "npm run lint:eslint -- --fix",
    "lint:tsc": "vue-tsc --build --force",
    "test:run": "vitest run",
    "test:run:coverage": "npm run test:run -- --coverage",
    "test:watch": "vitest",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/"
  },
  "dependencies": {
    "@vee-validate/yup": "^4.12.4",
    "@vueuse/core": "^10.7.1",
    "@vueuse/shared": "^10.7.1",
    "axios": "^1.6.5",
    "gsap": "^3.12.4",
    "pinia": "^2.1.7",
    "primeicons": "^6.0.1",
    "primevue": "^3.46.0",
    "vee-validate": "^4.12.4",
    "vue": "^3.4.5",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.6.1",
    "@tsconfig/node20": "^20.1.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^18.19.3",
    "@vitejs/plugin-vue": "^5.0.2",
    "@vitest/coverage-v8": "^1.1.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "jsdom": "^23.1.0",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.1.1",
    "typescript": "~5.3.3",
    "vite": "^5.0.11",
    "vitest": "^1.1.3",
    "vue-tsc": "^1.8.27"
  }
}
